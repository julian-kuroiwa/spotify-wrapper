{
  "name": "tdd-course",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "prepush": "npm run test:coverage",
    "test": "jest",
    "test:tdd": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "build": "npm run clear && ./node_modules/.bin/babel --out-dir lib src",
    "build:watch": "npm run clear && npm run build -- --watch --source-maps",
    "clear": "rimraf lib"
  },
  "jest": {
    "coverageReporters": [
      "text",
      "html"
    ],
    "coverageThreshold": {
      "global": {
        "functions": 80,
        "lines": 80
      }
    },
    "setupFiles": [
      "./setupJest.js"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/node": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "babel-jest": "^25.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.1",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "jest-fetch-mock": "^3.0.1",
    "node-fetch": "^2.6.0",
    "rimraf": "^3.0.2"
  },
  "dependencies": {
    "axios": "^0.19.2"
  }
}
